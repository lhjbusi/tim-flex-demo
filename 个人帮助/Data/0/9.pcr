#######################\par
### Transaction API ###\par
#######################\par
\par
## Enable automatic flush during the JTA beforeCompletion() callback\par
## (This setting is relevant with or without the Transaction API)\par
\par
#hibernate.transaction.flush_before_completion\par
\par
\par
## Enable automatic session close at the end of transaction\par
## (This setting is relevant with or without the Transaction API)\par
\par
#hibernate.transaction.auto_close_session\par
\par
\par
## the Transaction API abstracts application code from the underlying JTA or JDBC transactions\par
\par
#hibernate.transaction.factory_class org.hibernate.transaction.JTATransactionFactory\par
#hibernate.transaction.factory_class org.hibernate.transaction.JDBCTransactionFactory\par
\par
\par
## to use JTATransactionFactory, Hibernate must be able to locate the UserTransaction in JNDI\par
## default is java:comp/UserTransaction\par
## you do NOT need this setting if you specify hibernate.transaction.manager_lookup_class\par
\par
#jta.UserTransaction jta/usertransaction\par
#jta.UserTransaction javax.transaction.UserTransaction\par
#jta.UserTransaction UserTransaction\par
\par
\par
## to use the second-level cache with JTA, Hibernate must be able to obtain the JTA TransactionManager\par
\par
#hibernate.transaction.manager_lookup_class org.hibernate.transaction.JBossTransactionManagerLookup\par
#hibernate.transaction.manager_lookup_class org.hibernate.transaction.WeblogicTransactionManagerLookup\par
#hibernate.transaction.manager_lookup_class org.hibernate.transaction.WebSphereTransactionManagerLookup\par
#hibernate.transaction.manager_lookup_class org.hibernate.transaction.OrionTransactionManagerLookup\par
#hibernate.transaction.manager_lookup_class org.hibernate.transaction.ResinTransactionManagerLookup\par
\par
\par
\par
##############################\par
### Miscellaneous Settings ###\par
##############################\par
\par
## print all generated SQL to the console\par
\par
#hibernate.show_sql true\par
\par
\par
## format SQL in log and console\par
\par
hibernate.format_sql true\par
\par
\par
## add comments to the generated SQL\par
\par
#hibernate.use_sql_comments true\par
\par
\par
## generate statistics\par
\par
#hibernate.generate_statistics true\par
\par
\par
## auto schema export\par
\par
#hibernate.hbm2ddl.auto create-drop\par
#hibernate.hbm2ddl.auto create\par
#hibernate.hbm2ddl.auto update\par
#hibernate.hbm2ddl.auto validate\par
\par
\par
## specify a default schema and catalog for unqualified tablenames\par
\par
#hibernate.default_schema test\par
#hibernate.default_catalog test\par
\par
\par
## enable ordering of SQL UPDATEs by primary key\par
\par
#hibernate.order_updates true\par
\par
\par
## set the maximum depth of the outer join fetch tree\par
\par
hibernate.max_fetch_depth 1\par
\par
\par
## set the default batch size for batch fetching\par
\par
#hibernate.default_batch_fetch_size 8\par
\par
\par
## rollback generated identifier values of deleted entities to default values\par
\par
#hibernate.use_identifer_rollback true\par
\par
\par
## enable CGLIB reflection optimizer (enabled by default)\par
\par
#hibernate.cglib.use_reflection_optimizer false\par
\par
\par
\par
#####################\par
### JDBC Settings ###\par
#####################\par
\par
## specify a JDBC isolation level\par
\par
#hibernate.connection.isolation 4\par
\par
\par
## enable JDBC autocommit (not recommended!)\par
\par
#hibernate.connection.autocommit true\par
\par
\par
## set the JDBC fetch size\par
\par
#hibernate.jdbc.fetch_size 25\par
\par
\par
## set the maximum JDBC 2 batch size (a nonzero value enables batching)\par
\par
#hibernate.jdbc.batch_size 5\par
#hibernate.jdbc.batch_size 0\par
\par
\par
## enable batch updates even for versioned data\par
\par
hibernate.jdbc.batch_versioned_data true\par
\par
\par
## enable use of JDBC 2 scrollable ResultSets (specifying a Dialect will cause Hibernate to use a sensible default)\par
\par
#hibernate.jdbc.use_scrollable_resultset true\par
\par
\par
## use streams when writing binary types to / from JDBC\par
\par
hibernate.jdbc.use_streams_for_binary true\par
\par
\par
## use JDBC 3 PreparedStatement.getGeneratedKeys() to get the identifier of an inserted row\par
\par
#hibernate.jdbc.use_get_generated_keys false\par
\par
\par
## choose a custom JDBC batcher\par
\par
# hibernate.jdbc.factory_class\par
\par
\par
## enable JDBC result set column alias caching \par
## (minor performance enhancement for broken JDBC drivers)\par
\par
# hibernate.jdbc.wrap_result_sets\par
\par
\par
## choose a custom SQL exception converter\par
\par
#hibernate.jdbc.sql_exception_converter\par
\par
\par
\par
##########################\par
### Second-level Cache ###\par
##########################\par
\par
## optimize chache for minimal "puts" instead of minimal "gets" (good for clustered cache)\par
\par
#hibernate.cache.use_minimal_puts true\par
\par
\par
## set a prefix for cache region names\par
\par
hibernate.cache.region_prefix hibernate.test\par
\par
\par
## disable the second-level cache\par
\par
#hibernate.cache.use_second_level_cache false\par
\par
\par
## enable the query cache\par
\par
#hibernate.cache.use_query_cache true\par
\par
\par
## store the second-level cache entries in a more human-friendly format\par
\par
#hibernate.cache.use_structured_entries true\par
\par
\par
## choose a cache implementation\par
\par
#hibernate.cache.provider_class org.hibernate.cache.EhCacheProvider\par
#hibernate.cache.provider_class org.hibernate.cache.EmptyCacheProvider\par
hibernate.cache.provider_class org.hibernate.cache.HashtableCacheProvider\par
#hibernate.cache.provider_class org.hibernate.cache.TreeCacheProvider\par
#hibernate.cache.provider_class org.hibernate.cache.OSCacheProvider\par
#hibernate.cache.provider_class org.hibernate.cache.SwarmCacheProvider\par
\par
\par
## choose a custom query cache implementation\par
\par
#hibernate.cache.query_cache_factory\par
\par
\par
\par
############\par
### JNDI ###\par
############\par
\par
## specify a JNDI name for the SessionFactory\par
\par
#hibernate.session_factory_name hibernate/session_factory\par
## Hibernate uses JNDI to bind a name to a SessionFactory and to look up the JTA UserTransaction;\par
## if hibernate.jndi.* are not specified, Hibernate will use the default InitialContext() which\par
## is the best approach in an application server\par
\par
#file system\par
#hibernate.jndi.class com.sun.jndi.fscontext.RefFSContextFactory\par
#hibernate.jndi.url file:/\par
\par
#WebSphere\par
#hibernate.jndi.class com.ibm.websphere.naming.WsnInitialContextFactory\par
#hibernate.jndi.url iiop://localhost:900/\par
